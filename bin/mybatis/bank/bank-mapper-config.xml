<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>

	<settings>
		<setting name="cacheEnabled" value="true" />
		<setting name="lazyLoadingEnabled" value="false" />
		<setting name="useGeneratedKeys" value="true" />
		<setting name="logPrefix" value="database.mncg_rule" />
		<setting name="mapUnderscoreToCamelCase" value="true" />
	</settings>
	<typeAliases>
	    <typeAlias alias="Bank" type="com.youguu.asteroid.bank.pojo.Bank" />
	    <typeAlias alias="BankGroup" type="com.youguu.asteroid.bank.pojo.BankGroup" />
	</typeAliases>

	<mappers>
	  <mapper resource="mybatis/bank/bank_bank_mapper.xml" />
	  <mapper resource="mybatis/bank/bank_bankgroup_mapper.xml" />
	</mappers>
	
</configuration>
